<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anelcc.usercomunacation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.anelcc.usercomunacation.info.InfoActivity"></activity>
        <activity android:name="com.anelcc.usercomunacation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.anelcc.usercomunacation.toast.ToastActivity" />
        <activity android:name="com.anelcc.usercomunacation.snackbar.SnackbarActivity" />
        <activity android:name="com.anelcc.usercomunacation.dialog.DialogActivity" />
        <activity android:name="com.anelcc.usercomunacation.notification.NotificationActivity" />
        <activity android:name="com.anelcc.usercomunacation.notification.NotificationResultActivity" />
    </application>

</manifest>